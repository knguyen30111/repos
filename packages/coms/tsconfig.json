{
	"compilerOptions": {
		"outDir": "lib",
		"noImplicitAny": true,
		"target": "es5",
		"skipLibCheck": true,
		"lib": ["es5", "es6", "dom"],
		"types": ["react"],
		"jsx": "react",
		"allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop":true,
    // https://github.com/RyanCavanaugh/learn-a/#tsconfigsettingsjson
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
	},
}